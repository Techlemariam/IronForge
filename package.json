{
  "name": "ironforge-rpg",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "next lint",
    "agent:verify": "npm run lint && npm run test && npm run build",
    "agent:format": "npx prettier --write .",
    "agent:types": "npx prisma generate",
    "postinstall": "prisma generate",
    "test:docker:up": "docker-compose -f docker-compose.test.yml up -d --wait",
    "test:docker:down": "docker-compose -f docker-compose.test.yml down",
    "test:integration": "npm run test:docker:up && dotenv -e .env.test -- npx prisma db push --accept-data-loss && dotenv -e .env.test -- vitest run --config vitest.integration.config.ts",
    "docs:generate": "typedoc"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/react": "^3.0.3",
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@google/genai": "^1.33.0",
    "@mediapipe/tasks-vision": "0.10.9",
    "@prisma/client": "^6.0.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@serwist/next": "^9.3.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.10",
    "@types/canvas-confetti": "^1.9.0",
    "@types/howler": "^2.2.12",
    "ai": "^4.0.0",
    "axios": "^1.6.0",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "howler": "^2.2.4",
    "lucide-react": "^0.561.0",
    "next": "^15.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-player": "^2.16.1",
    "reactflow": "^11.10.4",
    "recharts": "^3.6.0",
    "serwist": "^9.3.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/three": "^0.182.0",
    "autoprefixer": "^10.4.14",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.1.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.27",
    "prisma": "^6.0.0",
    "tailwindcss": "^3.3.3",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.0.2",
    "vitest": "^4.0.16"
  }
}